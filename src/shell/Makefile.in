srcdir=		@srcdir@
top_srcdir=	@top_srcdir@
top_builddir=	@top_builddir@

include $(top_srcdir)/Mk/base.mk


PROGRAM=	shell
PROGRAM_DEPS=	$(top_builddir)/lib/sdi/libsdi.a \
		$(top_builddir)/lib/crt0/libcrt0.a \
		$(top_builddir)/lib/malloc_memoryserver/libmalloc_memoryserver.a \
		$(top_srcdir)/include/$(ARCH)/sdi.ld

SRCS=		main.cc \
		builtin_cat.cc \
		builtin_lookup.cc \
		builtin_ls.cc \
		builtin_uname.cc

IDLS=           

LIBS+=		-lsdi -lmalloc_memoryserver -ll4 -lsdi -lio
LDFLAGS+=	-Ttext=00100000 -T $(top_builddir)/include/$(ARCH)/sdi.ld $(top_builddir)/lib/crt0/libcrt0.a


include $(top_srcdir)/Mk/prog.mk
